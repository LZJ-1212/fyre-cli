{
  "name": "{% projectName %}",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "lint": "eslint . --ext js,jsx,vue --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,vue --fix",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build:analyze": "vite-bundle-analyzer",
    "preview:analyze": "npm run build && vite-bundle-analyzer"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "pinia": "^2.1.6",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "vite": "^4.4.5",
    "vitest": "^0.34.1",
    "@vitest/ui": "^0.34.1",
    "@vitest/coverage-v8": "^0.34.1",
    "jsdom": "^22.1.0",
    "@vue/test-utils": "^2.4.1",
    "vite-bundle-analyzer": "^1.0.2",
    "prettier": "^3.0.0",
    "vue-tsc": "^1.8.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}